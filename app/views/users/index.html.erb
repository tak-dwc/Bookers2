<%= render 'users/info', user: @user %>
<%= render 'books/new', book: @book %>

<h1>Users</h1>
<table>
  <thead>
  <tr>
     <th></th>  
     <th>image</th>
     <th>name</th>
   </tr>
  </thead>
  
  <tbody>
    <% @users.each do |user| %>
   <tr>
    <td><%= attachment_image_tag user, :profile_image, :fill, 50, 50,fallback: "no_image.jpg" %></td>
    <td><%= user.name %></td>
    <td><%=link_to "Show",user_path(user.id)%></td>
  </tr>
  
  <% end %>
    
 </tbody>
</table>

